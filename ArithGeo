def ArithGeo(arr)

  # code goes here
  i=1
   constant=arr[i]-arr[i-1]
   while i < arr.length
      break if arr[i]-arr[i-1] != constant
      return "Arithmetic" if i==arr.length-1
      i+=1
   end
   #geo test
   i=1
   constant=arr[i]/arr[i-1]
   while i < arr.length
      break if arr[i]/arr[i-1] != constant
      return "Geometric" if i==arr.length-1
      i+=1
   end
   return -1
end
   
